{# Functions #}
{% import _self as fn %}
{% macro propType(type) %}
{% spaceless %}
    {% if type == "double" %}
        number
    {% elseif type == "text" %}
        string
    {% else %}
        {{type}}
    {% endif %}
{% endspaceless %}
{% endmacro %}

export class {{entityName}} {

    id: number;
    {% for prop in properties %}

    {{prop.name}}: {{fn.propType(prop.type)}};
    {% endfor %}

}